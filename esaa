Declare  
myString varchar2(50); 
 FUNCTION changeString (str  Varchar2) 
return varchar2 is  
 Begin 
 myString := 'We study Programming Language'; 
 Return SUBSTR(myString, '0', '9') || str || SUBSTR(myString, '21', '29'); 
 end;
 Begin 
DBMS_OUTPUT.PUT_LINE(changeString('english')); 
end;

